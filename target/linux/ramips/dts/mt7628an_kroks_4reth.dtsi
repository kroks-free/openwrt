#include "mt7628an_kroks.dtsi"

#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio 38 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};
};

&esw {
	mediatek,portmap = <0x2f>;
	mediatek,portdisable = <0x21>;
};

&state_default {
	gpio_reset {
		groups = "wdt";
		function = "gpio";
	};

	p1led_an {
		groups = "p1led_an";
		function = "p1led_an";
	};

	p2led_an {
		groups = "p2led_an";
		function = "p2led_an";
	};

	p3led_an {
		groups = "p3led_an";
		function = "p3led_an";
	};

	p4led_an {
		groups = "p4led_an";
		function = "p4led_an";
	};
};
