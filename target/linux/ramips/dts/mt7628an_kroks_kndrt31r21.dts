#include "mt7628an_kroks_5eth.dtsi"

/ {
	compatible = "kroks,kndrt31r21", "mediatek,mt7628an-soc";
	model = "Kroks KNdRt31R21";

	aliases {
		led-boot = &led_power;
		led-failsafe = &led_power;
		led-running = &led_power;
		led-upgrade = &led_power;
	};

	leds {
		compatible = "gpio-leds";

		led_power: power {
			label = "kndrt31r21:led:power";
			gpios = <&gpio0 2 GPIO_ACTIVE_LOW>;
		};

		wifi {
			label = "kndrt31r21:led:wifi";
			gpios = <&gpio1 12 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};
	};

	gpio_export {
		compatible = "gpio-export";
		#size-cells = <0>;

		modem1power {
			gpio-export,name = "modem1power";
			gpio-export,output = <1>;
			gpios = <&gpio0 1 GPIO_ACTIVE_HIGH>;
		};

		modem2power {
			gpio-export,name = "modem2power";
			gpio-export,output = <1>;
			gpios = <&gpio0 3 GPIO_ACTIVE_HIGH>;
		};
	};
};

&state_default {
	gpio {
		ralink,group = "i2s", "wled_an";
		ralink,function = "gpio";
	};
};
